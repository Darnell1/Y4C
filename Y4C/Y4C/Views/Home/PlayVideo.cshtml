@model Y4C.Models.AddContent 

<!DOCTYPE html>
<html>
<head>
    <title>Y4C</title>
</head>
<body>

    <h1>@Model.Title</h1>
    <br />

    <iframe width="560"
            height="315"
            src="@Model.Video"
            frameborder="0"
            allowfullscreen></iframe><br />

    <iframe width="560"
            height="100"
            src="@Model.Audio"
            frameborder="0"
            allowfullscreen></iframe>
    <br />

    <p>@Model.Description</p>

    <hr />


    @Html.Raw(Model.BlogContent)
</body>
</html>

